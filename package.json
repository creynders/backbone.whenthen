{
  "name": "backbone.whenthen",
  "version": "1.0.0",
  "description": "When <events> then <execute> (for backbone)",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js tests",
    "qa": "npm run lint && npm run test-all",
    "test": "mocha tests/source.test.js",
    "test-all": "mocha tests/all.test.js --reporter dot",
    "build": "npm run lint && uglifyjs index.js -o dist/backbone.whenthen.js --beautify && uglifyjs index.js -o dist/backbone.whenthen.min.js -cm && npm run test-all;"
  },
  "author": "Camille Reynders",
  "license": "MIT",
  "devDependencies": {
    "backbone": "^1.1.2",
    "mocha": "^2.2.4",
    "must": "^0.12.0",
    "sinon": "^1.14.1",
    "uglify-js": "^2.4.20"
  },
  "dependencies": {
    "underscore": "^1.8.3"
  }
}
